{
	"version": "2.0.0",
	"tasks": [
		{
			"label": "Open Clean App (no-cache)",
			"type": "shell",
			"command": "powershell",
			"args": [
				"-NoProfile",
				"-ExecutionPolicy",
				"Bypass",
				"-Command",
				"Start-Process http://localhost:3000/clean/?reset=1&v=$(Get-Date -UFormat %s)"
			],
			"isBackground": false,
			"problemMatcher": [],
			"group": "none"
		}
	]
}